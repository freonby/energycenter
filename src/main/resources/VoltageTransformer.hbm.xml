<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="by.qlab.energycenter.datamodel.VoltageTransformer" table="VOLTAGETRANSFORMER">
        <id name="voltagetransformer_id" type="long">
            <column name="VOLTAGETRANSFORMER_ID" />
            <generator class="increment" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" />
        </property>
        <many-to-one name="fider" class="by.qlab.energycenter.datamodel.Fider" fetch="join">
            <column name="FIDER_ID" />
        </many-to-one>
        <property name="KU" type="int">
            <column name="KU" />
        </property>
        <list name="listVoltTransformerTypes" inverse="false" table="UTTYPE" lazy="true" cascade="all" fetch="select">
            <key>
                <column name="VOLTAGETRANSFORMER_ID" />
            </key>
            <list-index></list-index>
            <one-to-many class="by.qlab.energycenter.datamodel.UTType" />
        </list>
    </class>
</hibernate-mapping>
