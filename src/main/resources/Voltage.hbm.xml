<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="by.qlab.energycenter.datamodel.Voltage" table="VOLTAGE">
        <id name="voltage_id" type="long">
            <column name="VOLTAGE_ID" />
            <generator class="increment" />
        </id>
        <property name="voltage" type="java.lang.String">
            <column name="VOLTAGE" />
        </property>
    </class>
</hibernate-mapping>
