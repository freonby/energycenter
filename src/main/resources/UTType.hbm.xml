<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="by.qlab.energycenter.datamodel.UTType" table="UTTYPE">
        <id name="uttype_id" type="long">
            <column name="UTTYPE_ID" />
            <generator class="increment" />
        </id>
        <property name="type" type="java.lang.String">
            <column name="TYPE" />
        </property>
        <many-to-one name="voltageTransformer" class="by.qlab.energycenter.datamodel.VoltageTransformer" fetch="join">
            <column name="VOLTAGETRANSFORMER_ID" />
        </many-to-one>
    </class>
</hibernate-mapping>
