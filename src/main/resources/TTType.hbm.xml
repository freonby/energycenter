<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="by.qlab.energycenter.datamodel.TTType" table="TTTYPE">
        <id name="tttype_id" type="long">
            <column name="TTTYPE_ID" />
            <generator class="increment" />
        </id>
        <property name="type" type="java.lang.String">
            <column name="TYPE" />
        </property>
        <many-to-one name="currentTransformer" class="by.qlab.energycenter.datamodel.CurrentTransformer" fetch="join">
            <column name="CURRENTTRANSFORMER_ID" />
        </many-to-one>
    </class>
</hibernate-mapping>
