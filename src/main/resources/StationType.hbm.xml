<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="by.qlab.energycenter.datamodel.StationType" table="STATIONTYPE">
        <id name="stationtype_id" type="long">
            <column name="STATIONTYPE_ID" />
            <generator class="increment" />
        </id>
        <property name="typeStation" type="java.lang.String">
            <column name="TYPESTATION" />
        </property>
        <many-to-one name="transformerStation" class="by.qlab.energycenter.datamodel.TransformerStation" fetch="join">
            <column name="TRANSFORMERSTATION_ID" />
        </many-to-one>
    </class>
</hibernate-mapping>
