<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="by.qlab.energycenter.datamodel.CurrentTransformer" table="CURRENTTRANSFORMER">
        <id name="currenttransformer_id" type="long">
            <column name="CURRENTTRANSFORMER_ID" />
            <generator class="increment" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" />
        </property>
        <many-to-one name="fider" class="by.qlab.energycenter.datamodel.Fider" fetch="join">
            <column name="FIDER_ID" />
        </many-to-one>
        <property name="KI" type="int">
            <column name="KI" />
        </property>
        <list name="listCurTransformerTypes" inverse="false" table="TTTYPE" lazy="true" cascade="all" fetch="select">
            <key>
                <column name="CURRENTTRANSFORMER_ID" />
            </key>
            <list-index></list-index>
            <one-to-many class="by.qlab.energycenter.datamodel.TTType" />
        </list>
    </class>
</hibernate-mapping>
