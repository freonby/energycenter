<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="by.qlab.energycenter.datamodel.EnergyMeterType" table="ENERGYMETERTYPE">
        <id name="energymetertype_id" type="long">
            <column name="ENERGYMETERTYPE_ID" />
            <generator class="increment" />
        </id>
        <property name="type" type="java.lang.String">
            <column name="TYPE" />
        </property>
    </class>
</hibernate-mapping>
